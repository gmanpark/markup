@charset "utf-8";
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0}
body,input,textarea,select,button,table{font-family:"돋움",Dotum, Helvetica,sans-serif;font-size:12px}
img,fieldset{border:0}
ul,ol{list-style:none}
em,address{font-style:normal}
a{display:block;text-decoration:underline}
a:hover,a:active,a:focus{text-decoration:underline}
.blind{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:1px;height:1px;font-size:0;line-height:0}
.content{width:729px;margin:0 30px;padding:30px 0 20px}
span{display:inline-block;width:550px}
.article_item{clear:both;line-height:19px;margin:10px}
h2{font-size:16px;line-height:24px;font-family:"나눔고딕", sans-serif}
div{font-family:Gulim;font-size:13px}
img{float:left;margin-right:10px;margin-bottom:10px}
h1{width:100%;color:#fff;font-size:18px;line-height:24px;text-indent:15px;background:#000;font-family:"나눔고딕", sans-serif;margin:0;padding:13px 0 11px}
#footer *{clear:both;text-align:center;color:#444;list-style:none;font:10px Verdana;line-height:normal;border-top:1px solid #ddd;margin:0;padding:10px}
#footer address strong{font-weight:700;font-family:Tahoma;color:#00535d}