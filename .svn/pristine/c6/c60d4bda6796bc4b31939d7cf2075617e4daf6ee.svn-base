@charset "utf-8";
/* NHN Web Standard 1Team JJS 090707, JJJ 090815 */

/* common */
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0}
body,input,textarea,select,button,table{font-family:'돋움',Dotum, Helvetica,sans-serif;font-size:12px}
img,fieldset{border:0}
ul,ol{list-style:none}
em,address{font-style:normal}
a{text-decoration:none}
a:hover,a:active,a:focus{text-decoration:underline}
.blind{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:1px;height:1px;font-size:0;line-height:0}

/* nts header footer */
h1{width:100%;margin:0;padding:13px 0 11px;background:#000;font-size:18px;line-height:24px;font-family:'나눔고딕',sans-serif;color:#fff;text-indent:15px}
h2{font-size:16px;font-family:'나눔고딕',sans-serif}
#footer{clear:both;margin:0;padding:10px;border-top:1px solid #ddd;font:10px Verdana;line-height:normal;color:#444;text-align:center;list-style:none}
#footer address strong{font-weight:700;font-family:Tahoma,Geneva,sans-serif;color:#00535d}

/* olympic article */
#content{margin:0 auto;width:660px;font-family: "돋움", Dotum, Helvetica, sans-serif; font-size: 12px; color: #666666;line-height:13px}
.article_head{border-bottom:1px solid #e9e9e9}
.article_title{display:block;margin:36px auto 20px auto;width:660px;height:90px;background:url(../img/sp_problem1.png) 0 0 no-repeat;text-indent: -999em}
.current_medal{margin:0 auto 30px auto;width:660px;height:40px;background:url(../img/sp_problem1.png) 0 -100px no-repeat;font-family:verdana,sans-serif;font-size:15px;font-weight:bold;color:white;text-align:left;}
.medal_gold{display:inline-block;position:relative;padding:15px 0 0 192px}
.medal_silver{display:inline-block;position:relative;padding:15px 0 0 112px}
.medal_bronze{display:inline-block;position:relative;padding:15px 0 0 110px}
.sub_title{display:inline-block;margin:40px 0 24px;width:100%;height:30px;background:url(../img/sp_problem1.png) 0 -150px no-repeat;text-indent:-999em}
.sub_content{margin:0 0 40px;line-height:24px;text-align:justify}
.sub_title_v2{margin:0 0 20px;width:100%;height:25px;background:url(../img/sp_problem1.png) 0 -190px no-repeat;text-indent:-999em}
.sub_content_v2{margin:0 0 23px;line-height:24px;text-align:justify}
.fencing_title{display:block;margin:12px 0 5px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -230px no-repeat}
.swim_title{display:block;margin:21px 0 5px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -280px no-repeat}
.horse_title{display:block;margin:12px 0 5px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -330px no-repeat}
.combine_title{display:block;margin:23px 0 7px;width:140px;height:18px;background:url(../img/sp_problem1.png) -400px -380px no-repeat}
.img_fencing{display:inline-block;margin:17px 28px 17px 15px;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -220px no-repeat}
.img_swim{display:inline-block;margin:17px 28px 17px 15px;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -270px no-repeat}
.img_horse{display:inline-block;margin:17px 28px 17px 15px;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -320px no-repeat}
.img_combine{display:inline-block;margin:17px 28px 17px 15px;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -370px no-repeat}
.article_desc{overflow:hidden;margin-bottom:50px;border-top:2px solid #e9e9e9;border-bottom:2px solid #e9e9e9}
.article_desc dt{display:inline-block;float:left;}
.article_desc dd{float:left;height:78px;line-height:19px;}
.article_desc .line_horizon {display:block;width:100%;height:0;border-bottom:1px solid #e9e9e9;}