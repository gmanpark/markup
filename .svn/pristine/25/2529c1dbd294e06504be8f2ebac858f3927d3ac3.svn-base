h2 {
    font-size: 16px;
    line-height: 24px;
    font-family: "나눔고딕", sans-serif;
}

/* Header */
h1 {
    width: 100%;
    margin: 0;
    padding: 13px 0 11px;
    color: #fff;
    font-size: 18px;
    line-height: 24px;
    text-indent: 15px;
    background: #000;
    font-family: "나눔고딕", sans-serif;
}

/* Footer */
#footer * {
    clear: both;
    text-align: center;
    margin: 0;
    padding: 10px;
    color: #444;
    list-style: none;
    font: 10px Verdana;
    line-height: normal;
    border-top: 1px solid #ddd;
}

#footer address strong {
    font-weight: bold;
    font-family: Tahoma;
    color: #00535d;
}