    width: 230px;
    font-family: '돋움', dotum, applegothic, sans-serif;
    font-size: 12px;
    color: #002a5b;
    background: url('../img/2_0.gif') no-repeat bottom;
    list-style-position: inside;
    line-height: 20px;

}

