@charset "utf-8";
/* common */body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0}
body,input,textarea,select,button,table{font-family:'돋움',Dotum,Helvetica,sans-serif;font-size:12px}
img,fieldset{border:0}
ul,ol{list-style:none}
em,address{font-style:normal}
a{text-decoration:none}
a:hover,a:active,a:focus{text-decoration:underline}
.blind{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:1px;height:1px;font-size:0;line-height:0}
.content{width:729px;margin:0 30px;padding:30px 0 20px 10px}

/* nts header footer */
h1{width:100%;margin:0;padding:13px 0 11px;background:#000;font-size:18px;line-height:24px;font-family:'나눔고딕',sans-serif;color:#fff;text-indent:15px}
h2{font-size:16px;line-height:24px;font-family:'나눔고딕',sans-serif}
#footer{clear:both;margin:0;padding:10px;border-top:1px solid #ddd;font:10px Verdana;line-height:normal;color:#444;text-align:center;list-style:none}
#footer address strong{font-weight:700;font-family:Tahoma,Geneva,sans-serifahoma;color:#00535d}

/* ranklist */
a{display:inline-block;width:140px;padding-left:5px}
.ordered_list{margin:2px;padding:5px 0}
.ordered_list li{margin:10px}
.list_rank{float:left;margin:0 0 20px 10px;background:url(../img/2_0.gif) repeat-y top}
.list_rank ol{width:230px;margin-bottom:-10px;background:url(../img/2_0.gif) repeat-y bottom;font-family:'돋움',dotum,applegothic,sans-serif;font-size:12px;color:#002a5b}
.rank_bg{position:relative;top:2px;background:url(../img/spr_problem2.png) no-repeat}
.rank1{display:inline-block;width:15px;height:13px;background-position:0 0}
.rank2{display:inline-block;width:15px;height:13px;margin-bottom:11px;background-position:-20px 0}
.rank3{display:inline-block;width:15px;height:13px;background-position:0 -20px}
.rank4{display:inline-block;width:15px;height:13px;background-position:-20px -20px}
.rank5{display:inline-block;width:15px;height:13px;background-position:-40px 0}
.rank6{display:inline-block;width:15px;height:13px;background-position:-40px -20px}
.rank7{display:inline-block;width:15px;height:13px;background-position:0 -40px}
.rank8{display:inline-block;width:15px;height:13px;background-position:-20px -40px}
.rank9{display:inline-block;width:15px;height:13px;background-position:-40px -40px}
.rank10{display:inline-block;width:15px;height:13px;background-position:-60px 0}
.rank_up{display:inline-block;width:7px;height:10px;background-position:0 -60px}
.new{display:inline-block;float:right;width:24px;height:9px;background-position:-10px -60px}
.rank_hit{display:inline-block;width:30px;font-family:Tahoma,Geneva,sans-serifahoma;font-size:11px;color:#5b5b5b;text-align:center}