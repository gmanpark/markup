@charset "utf-8";
/* NHN Web Standard 1Team JJS 090707, JJJ 090815 */

/* common */
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0}
body,input,textarea,select,button,table{font-family:'돋움',Dotum, Helvetica,sans-serif;font-size:12px}
img,fieldset{border:0}
ul,ol{list-style:none}
em,address{font-style:normal}
a{text-decoration:none}
a:hover,a:active,a:focus{text-decoration:underline}
.blind{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:1px;height:1px;font-size:0;line-height:0}

/* nts header footer */
h1{width:100%;margin:0;padding:13px 0 11px;background:#000;font-size:18px;line-height:24px;font-family:'나눔고딕',sans-serif;color:#fff;text-indent:15px}
h2{font-size:16px;font-family:'나눔고딕',sans-serif}
#footer{clear:both;margin:0;padding:10px;border-top:1px solid #ddd;font:10px Verdana;line-height:normal;color:#444;text-align:center;list-style:none}
#footer address strong{font-weight:700;font-family:Tahoma,Geneva,sans-serif;color:#00535d}

/* olympic article */
#content{margin:0 auto;width:666px;text-align:center;font-family: "돋움", Dotum, Helvetica, sans-serif; font-size: 12px; color: #666666;line-height:13px}
.article_head{border-bottom:1px solid #e9e9e9;text-align:center}
.article_title{margin:36px auto 30px auto;width:666px;height:91px;background:url(../img/sp_problem1.png) 0 0 no-repeat}
.current_medal{margin:0 auto 30px auto;width:666px;height:41px;background:url(../img/sp_problem1.png) 0 -100px no-repeat;font-family:Tahoma, sans-serif;font-size:20px;font-weight:bold;color:white}
.medal_gold{display:block;position:absolute;text-align:left;padding:15px 190px}
.medal_silver{display:block;position:absolute;text-align:left;padding:15px 320px}
.medal_bronze{display:block;position:absolute;text-align:left;padding:15px 445px}
.sub_title{display:inline-block;margin:40px 0 30px;width:666px;height:30px;background:url(../img/sp_problem1.png) 0 -150px no-repeat}
.sub_content{margin:0 0 50px;line-height:25px;text-align:justify}
.sub_title_v2{margin:0 0 30px;width:242px;height:25px;background:url(../img/sp_problem1.png) 0 -190px no-repeat}
.sub_content_v2{margin:0 0 30px;line-height:25px;text-align:justify}
.article_desc {overflow:hidden;margin-bottom:50px;border-top:2px solid #e9e9e9;border-bottom:2px solid #e9e9e9}
.article_desc li{float:left;width:100%;height:79px;border-bottom:1px solid #e9e9e9}
.item_img{float:left;overflow:hidden;padding:17px 26px 17px 15px}
.item_desc{float:left;height:100%}
.item_desc span{display:block;text-align:left;line-height:19px;color:#999999}
.fencing_title{display:block;margin:14px 0 7px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -230px no-repeat}
.swim_title{display:block;margin:23px 0 7px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -280px no-repeat}
.horse_title{display:block;margin:14px 0 7px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -330px no-repeat}
.combine_title{display:block;margin:23px 0 7px;width:32px;height:18px;background:url(../img/sp_problem1.png) -400px -380px no-repeat}
.img_fencing{display:block;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -220px no-repeat}
.img_swim{display:block;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -270px no-repeat}
.img_horse{display:block;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -320px no-repeat}
.img_combine{display:block;width:72px;height:40px;background:url(../img/sp_problem1.png) 0 -370px no-repeat}
